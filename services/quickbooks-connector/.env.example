# QuickBooks Connector env example
PORT=3010
NODE_ENV=development
# QuickBooks OAuth
QBO_CLIENT_ID=your_quickbooks_client_id
QBO_CLIENT_SECRET=your_quickbooks_client_secret
QBO_REDIRECT_URI=http://localhost:3010/oauth/callback
# Nexus API Gateway (where connector will post data)
NEXUS_GATEWAY_URL=http://localhost:3000
NEXUS_SERVICE_ACCOUNT_TOKEN=replace_with_service_token
# Mock mode (set to true to bypass real QuickBooks calls)
MOCK_MODE=true
